	<!-- handle gear commands -->
		
		<cmd input="/main">
	<if advanced="$Main=0">
	<setvar name="Main" value="1" />
	<setvar name="TH" value="0" />
	<equip set="Main" />
	<command>/echo Equipped Main set.</command>
