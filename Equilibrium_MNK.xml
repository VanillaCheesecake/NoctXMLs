	<!-- Auto WS use. -->
		
	<if advanced="$Autows=1">
		<if p_status="engaged">
			<if p_tp=">999">
				<if t_distance="<4.5">
			<command>/ws "$ws" &lt;t&gt;</command>
			</if>
			</if>
		</if>
	</if>
	
