	<!-- handle gear commands -->
		
		<cmd input="/main">
	<if advanced="$Main=0">
	<setvar name="Main" value="1" />
	<setvar name="TH" value="0" />
	<equip set="Main" />
	<command>/echo Equipped Main set.</command>
	</if>
	
	<else>
	<setvar name="Main" value="0" />
	<command>/echo Removed Main set.</command>
	<equip set="Default" />
	</else>
	</cmd>
	
	<cmd input="/th">
	<if advanced="$TH=0">
	<setvar name="TH" value="1" />
	<setvar name="Main" value="0" />
	<equip set="TH" />
	<command>/echo Equipped TH set.</command>
	</if>
	
	<else>
	<setvar name="TH" value="0" />
	<command>/echo Removed TH set.</command>
	<equip set="Default" />
	</else>
	</cmd>
	
	<cmd input="//th">
	<if advanced="$TH=0">
	<setvar name="TH" value="1" />
	<setvar name="Main" value="0" />
	<equip set="TH" />
	<command>/echo Equipped TH set.</command>
	</if>
	
	<else>
	<setvar name="TH" value="0" />
	<command>/echo Removed TH set.</command>
	<equip set="Default" />
	</else>
	</cmd>

		<cmd input="/autoth">
	<if advanced="$AutoTH=1">
	<setvar name="AutoTH" value="0" />
	<command>/echo Will no longer automatically equip TH set upon engaging in melee range for 3 seconds. Toggle on with /autoth.</command>
	</if>
	
	<else>
	<setvar name="AutoTH" value="1" />
	<command>/echo Will automatically equip TH set upon first engaging in melee range for 3 seconds. Toggle off with /autoth.</command>
	</else>
	</cmd>

	<!-- end of handle gear commands -->
	
